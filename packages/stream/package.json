{
    "name": "@nftcom/stream",
    "version": "1.0.0",
    "description": "Stream utilities",
    "main": "src/main",
    "types": "src/main",
    "scripts": {
        "build": "npm run clean && npm run compile",
        "clean": "rimraf dist tsconfig.tsbuildinfo",
        "compile": "tsc -p tsconfig.app.json && tsc-alias -p tsconfig.app.json",
        "dev": "doppler run -- concurrently -k -r \"nodemon | pino-pretty\"",
        "start": "node ./dist/src/main"
    },
    "dependencies": {
        "@alch/alchemy-web3": "^1.4.6",
        "@aws-sdk/lib-storage": "^3.135.0",
        "@babel/runtime": "^7.18.3",
        "@opensea/stream-js": "^0.0.25",
        "aws-sdk": "^2.1269.0",
        "axios-retry": "^3.4.0",
        "bullmq": "^3.10.1",
        "dotenv": "^10.0.0",
        "express": "^4.18.1",
        "ioredis": "^5.2.3",
        "keyv": "^4.3.0",
        "kill-port": "^1.6.1",
        "lodash": "^4.17.21",
        "multer": "^1.4.5-lts.1",
        "qs": "^6.11.0",
        "tar": "^6.1.13",
        "ws": "^8.12.1",
        "zod": "^3.19.1"
    },
    "devDependencies": {
        "@types/bull": "^3.15.7",
        "@types/multer": "^1.4.5-lts.1",
        "concurrently": "^6.3.0",
        "nodemon": "^2.0.14",
