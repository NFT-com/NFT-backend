{
  "name": "@nftcom/gql",
  "version": "1.0.0",
  "description": "NFT.com's GraphQL server",
  "main": "dist/index",
  "types": "dist/index",
  "scripts": {
    "prebuild": "npm run gql:gen",
    "build": "npm run clean && npm run compile && npm run copy:schema",
    "clean": "rimraf dist tsconfig.build.tsbuildinfo",
    "compile": "tsc -p tsconfig.build.json && tsc-alias",
    "copy:schema": "cp -r ./src/schema/*.graphql ./dist/schema/",
    "dev": "doppler run -- concurrently -k -r \"npm:gql:gen -- --watch\" \"nodemon | pino-pretty\"",
    "test:local": "LOG_LEVEL=silent doppler run --preserve-env -- jest --runInBand --detectOpenHandles",
    "test": "jest --coverage --runInBand",
    "test:openhandles": "jest --detectOpenHandles",
    "gql:gen": "graphql-codegen",
    "signature:gen": "ts-node script/gen-signature.ts",
    "start": "node -r source-map-support/register ./dist/index",
    "gqldoc": "npx spectaql ./doc-config.yml -t ../../docs/public -D",
    "gqldoc-old": "graphdoc -e http://localhost:10010/graphql -o ./../../docs/documentation -f"
  },
  "dependencies": {
    "@alch/alchemy-web3": "^1.4.6",
    "@apollo/utils.keyvadapter": "^1.1.2",
    "@aws-sdk/client-s3": "^3.186.0",
    "@aws-sdk/client-sts": "^3.131.0",
    "@aws-sdk/lib-storage": "^3.186.0",
    "@aws-sdk/types": "^3.127.0",
    "@graphql-tools/merge": "^8.2.0",
    "@hashgraph/sdk": "^2.18.3",
    "@keyv/redis": "^2.5.1",
    "@opentelemetry/exporter-trace-otlp-http": "0.31.0",
    "@opentelemetry/instrumentation-express": "0.30.0",
    "@opentelemetry/instrumentation-graphql": "0.29.0",
    "@opentelemetry/instrumentation-http": "0.31.0",
    "@opentelemetry/instrumentation-ioredis": "0.30.0",
    "@opentelemetry/instrumentation-pg": "0.30.0",
    "@opentelemetry/sdk-node": "0.31.0",
    "@opentelemetry/sdk-trace-base": "1.5.0",
    "@sendgrid/mail": "^7.7.0",
    "@sentry/node": "^6.19.7",
    "@sentry/tracing": "^6.19.7",
    "apollo-server-core": "^3.10.2",
    "apollo-server-express": "^3.10.2",
    "axios": "^0.21.4",
    "axios-retry": "^3.3.1",
    "bull": "^4.10.0",
    "cors": "^2.8.5",
    "crypto-random-string": "^3.3.1",
    "date-fns": "^2.29.3",
    "delay": "^5.0.0",
    "dotenv": "^10.0.0",
    "envfile": "^6.18.0",
    "ethereumjs-abi": "^0.6.8",
    "ethers": "5.6.4",
    "express": "^4.18.1",
    "get-stream": "^6.0.1",
    "graphql": "^15.8.0",
    "graphql-rate-limit-directive": "2.0.0",
    "graphql-resolvers": "^0.4.2",
    "graphql-scalars": "^1.18.0",
    "graphql-upload": "^13.0.0",
    "image-to-base64": "^2.2.0",
    "ioredis": "^5.2.3",
    "joi": "^17.6.2",
    "keyv": "^4.5.0",
    "kill-port": "^1.6.1",
    "lodash": "^4.17.21",
    "node-fetch": "^2.6.7",
    "path": "^0.12.7",
    "pino-http": "8.2.0",
    "source-map-support": "^0.5.21",
    "typeorm": "^0.3.10",
    "typesense": "^1.4.1",
    "web3": "^1.8.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.13.5",
    "@graphql-codegen/typescript": "^2.7.3",
    "@types/bull": "^3.15.9",
    "@types/graphql-resolvers": "^0.3.1",
    "@types/jest": "^28.1.8",
    "@types/lodash": "^4.14.186",
    "@types/node": "^16.11.64",
    "@types/supertest": "^2.0.12",
    "concurrently": "^6.5.1",
    "graphql-markdown": "^6.1.0",
    "jest": "^28.1.3",
    "nodemon": "^2.0.20",
    "pino-pretty": "9.1.0",
    "rimraf": "^3.0.2",
    "spectaql": "^1.5.4",
    "supertest": "^6.3.0",
    "ts-jest": "^28.0.8",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.7.0",
    "tsconfig-paths": "^3.14.1",
    "typescript": "^4.8.4"
  },
  "exports": {
    "./service": "./dist/service/index.js"
  },
  "typesVersions": {
    "*": {
      "service": [
        "dist/service/index.ts"
      ]
    }
  }
}
