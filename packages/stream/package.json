{
    "name": "@nftcom/stream",
    "version": "1.0.0",
    "description": "Streams/listeners/cronjobs",
    "main": "src/main",
    "types": "src/main",
    "scripts": {
        "build": "pnpm clean && pnpm compile",
        "postbuild": "./scripts/postbuild.sh",
        "clean": "rimraf dist tsconfig.tsbuildinfo",
