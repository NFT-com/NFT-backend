{
  "name": "@nftcom/contract-data",
  "version": "0.0.0",
  "private": true,
  "main": "src/index",
  "scripts": {
    "build": "tsup src/index.ts --format cjs",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "dev": "tsup src/index.ts --format cjs --watch --onSuccess \"node dist/index.js\"",
    "lint": "tsc --noEmit && eslint \"src/**/*.ts*\"",
    "start": "node dist/index.js",
    "test": "jest --detectOpenHandles"
  },
  "dependencies": {
    "@nftcom/cache": "workspace:*",
    "@nftcom/nftport-client": "workspace:*",
    "@nftcom/shared": "workspace:*",
    "crypto": "^1.0.1",
    "date-fns": "^2.29.3",
    "ethers": "^5",
    "ioredis": "^5.3.2",
    "lodash": "^4.17.21",
    "sinon": "^15.0.3",
    "typeorm": "^0.3.15"
  },
  "devDependencies": {
    "jest": "^29.5.0",
    "tsc": "^2.0.4",
    "tsup": "^6.7.0"
  }
}
