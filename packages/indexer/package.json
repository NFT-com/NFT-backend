{
  "name": "@nftcom/indexer",
  "version": "1.0.0",
  "description": "NFT.com's indexer for NFT data",
  "main": "dist/index",
  "types": "dist/index",
  "scripts": {
    "build": "npm run clean && npm run compile",
    "clean": "rimraf dist tsconfig.tsbuildinfo",
    "compile": "tsc && tsc-alias",
    "dev": "concurrently -k -r \"nodemon\"",
    "signature:gen": "ts-node script/gen-signature.ts",
    "start": "node -r source-map-support/register ./dist/index"
  },
  "dependencies": {
    "axios": "^0.24.0",
    "base-64": "^1.0.0",
    "crypto-random-string": "^3.3.1",
    "date-fns": "^2.25.0",
    "delay": "^5.0.0",
    "dotenv": "^10.0.0",
    "envfile": "^6.17.0",
    "ethers": "^5.5.2",
    "express": "^4.17.1",
    "is-base64": "^1.1.0",
    "is-ipfs": "^6.0.2",
    "is-url": "^1.2.4",
    "joi": "^17.4.2",
    "kill-port": "^1.6.1",
    "lodash": "^4.17.21",
    "node-cron": "^3.0.0",
    "source-map-support": "^0.5.21",
    "typeorm": "^0.2.41"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.176",
    "@types/node": "^16.11.7",
    "concurrently": "^6.3.0",
    "nodemon": "^2.0.14",
    "rimraf": "^3.0.2",
    "ts-node": "^10.4.0",
    "tsc-alias": "^1.4.1",
    "tsconfig-paths": "^3.11.0",
    "typescript": "^4.4.4"
  }
}
