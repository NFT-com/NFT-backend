{
  "name": "@nftcom/gql",
  "version": "1.0.0",
  "description": "NFT.com's GraphQL server",
  "main": "dist/packages/gql/src/index",
  "types": "dist/packages/gql/src/index",
  "scripts": {
    "prebuild": "npm run gql:gen",
    "build": "npm run clean && npm run compile && npm run copy:schema",
    "clean": "rimraf dist tsconfig.app.tsbuildinfo",
    "compile": "tsc -p tsconfig.app.json && tsc-alias -p tsconfig.app.json",
    "copy:schema": "cp -r ./src/schema/*.graphql ./dist/packages/gql/src/schema/",
    "dev": "doppler run -- concurrently -k -r \"npm:gql:gen -- --watch\" \"nodemon | pino-pretty\"",
    "dev:local": "concurrently -k -r \"npm:gql:gen -- --watch\" \"nodemon | pino-pretty\"",
    "test:local": "LOG_LEVEL=debug doppler run --preserve-env -- jest --runInBand --detectOpenHandles",
    "test:local-file": "LOG_LEVEL=debug doppler run --preserve-env -- jest --runInBand --detectOpenHandles --testPathPattern=test/resolver.txActivity.resolver.int.spec.ts",
    "test": "jest --coverage --runInBand",
    "test:openhandles": "jest --detectOpenHandles",
    "gql:gen": "graphql-codegen",
    "signature:gen": "ts-node script/gen-signature.ts",
    "start": "node -r source-map-support/register ./dist/packages/gql/src/index",
    "gqldoc": "npx spectaql ./doc-config.yml -t ../../docs/public -D",
    "gqldoc-old": "graphdoc -e http://localhost:10010/graphql -o ./../../docs/documentation -f"
  },
  "dependencies": {
    "@alch/alchemy-web3": "^1.4.7",
    "@apollo/utils.keyvadapter": "^1.1.2",
    "@aws-sdk/client-s3": "^3.315.0",
    "@aws-sdk/client-sts": "^3.131.0",
    "@aws-sdk/lib-storage": "^3.315.0",
    "@aws-sdk/types": "^3.127.0",
    "@ethersproject/abi": "^5.7.0",
    "@ethersproject/contracts": "^5.7.0",
    "@graphql-tools/merge": "^8.4.1",
    "@hashgraph/sdk": "^2.24.0",
    "@keyv/redis": "^2.5.7",
    "@opentelemetry/exporter-trace-otlp-http": "0.34.0",
    "@opentelemetry/instrumentation-express": "0.32.0",
    "@opentelemetry/instrumentation-graphql": "0.33.0",
    "@opentelemetry/instrumentation-http": "0.34.0",
    "@opentelemetry/instrumentation-ioredis": "0.33.0",
    "@opentelemetry/instrumentation-pg": "0.33.0",
    "@opentelemetry/sdk-trace-base": "1.8.0",
    "@opentelemetry/sdk-trace-node": "1.8.0",
    "@sendgrid/mail": "^7.7.0",
    "@sentry/node": "^6.19.7",
    "@sentry/tracing": "^6.19.7",
    "apollo-server-core": "^3.12.0",
    "apollo-server-express": "^3.12.0",
    "axios": "^0.21.4",
    "axios-retry": "^3.4.0",
    "big-number": "^2.0.0",
    "bull": "^4.10.4",
    "cors": "^2.8.5",
    "crypto-random-string": "^3.3.1",
    "dataloader": "^2.1.0",
    "date-fns": "^2.29.3",
    "delay": "^5.0.0",
    "dotenv": "^10.0.0",
    "envfile": "^6.18.0",
    "ethereumjs-abi": "^0.6.8",
    "express": "^4.18.2",
    "express-rate-limit": "^6.7.0",
    "get-stream": "^6.0.1",
    "graphql": "^16.6.0",
    "graphql-rate-limit-directive": "^2.0.3",
    "graphql-resolvers": "^0.4.2",
    "graphql-scalars": "^1.21.3",
    "graphql-upload": "^15.0.2",
    "image-to-base64": "^2.2.0",
    "ioredis": "^5.3.2",
    "isomorphic-unfetch": "^3.1.0",
    "joi": "^17.9.1",
    "keyv": "^4.5.2",
    "kill-port": "^1.6.1",
    "lodash": "^4.17.21",
    "lru_map": "^0.4.1",
    "node-fetch": "^2.6.9",
    "path": "^0.12.7",
    "pino-http": "^8.3.3",
    "qs": "^6.11.1",
    "rate-limiter-flexible": "^2.4.1",
    "source-map-support": "^0.5.21",
    "typeorm": "^0.3.15",
    "typesense": "^1.5.3",
    "url-encode-decode": "^1.0.0",
    "web3": "^1.9.0",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.16.5",
    "@graphql-codegen/typescript": "^2.8.8",
    "@types/bull": "^3.15.9",
    "@types/graphql-resolvers": "^0.3.1",
    "@types/jest": "^28.1.1",
    "@types/lodash": "^4.14.194",
    "@types/node": "^16.11.7",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "concurrently": "^6.5.1",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "graphql-markdown": "^6.1.0",
    "jest": "^28.1.1",
    "nodemon": "^2.0.22",
    "pino-pretty": "9.1.0",
    "rimraf": "^3.0.2",
    "supertest": "^6.3.3",
    "ts-jest": "^28.0.5",
    "ts-node": "^10.4.0",
    "tsc-alias": "^1.8.5",
    "tsconfig-paths": "^3.14.2",
    "typescript": "^4.9.5"
  },
  "exports": {
    "./service": "./dist/packages/gql/src/service/index.js"
  },
  "typesVersions": {
    "*": {
      "service": [
        "dist/packages/gql/src/service/index.ts"
      ]
    }
  }
}
