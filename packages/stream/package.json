        "@opensea/stream-js": "^0.0.25",
        "axios": "^0.21.4",
        "axios-retry": "^3.4.0",
        "bullmq": "^3.10.1",
        "dd-trace": "^3.15.0",
        "dotenv": "^10.0.0",
        "ethers": "^5.5.1",
        "express": "^4.18.1",
        "ioredis": "^5.2.3",
        "kill-port": "^1.6.1",
        "lodash": "^4.17.21",
        "multer": "^1.4.5-lts.1",
        "node-fetch": "^2.6.7",
        "pg": "^8.8.0",
