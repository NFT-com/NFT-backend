{
  "name": "@nftcom/shared",
  "main": "./src/index",
  "source": "./src/index",
  "types": "./src/index",
  "scripts": {
    "prebuild": "pnpm clean",
    "build": "pnpm compile",
    "postbuild": "./scripts/postbuild.sh",
    "clean": "rm -rf ../../dist/shared",
    "dev": "tsc -w",
    "lint": "eslint \"src/**/*.ts*\"",
    "compile": "tsc && tsc-alias",
    "typeorm:cli": "TS_NODE_TRANSPILE_ONLY=true node -r tsconfig-paths/register -r ts-node/register ../../node_modules/typeorm/cli",
    "typeorm:gen": "pnpm typeorm:cli -- migration:generate -d ./src/db/data-source.ts",
    "typeorm:migration:run": "pnpm typeorm:cli -- migration:run -d ./src/db/data-source.ts",
    "typeorm:migration:create": "pnpm typeorm:cli -- migration:create -n",
    "typeorm:revert": "pnpm typeorm:cli --  migration:revert -d ./src/db/data-source.ts",
    "typeorm:show": "pnpm typeorm:cli -- migration:show -d ./src/db/data-source.ts",
    "typeorm:sync": "pnpm typeorm:cli -- schema:sync",
    "typechain": "typechain --target=ethers-v5 ./src/helper/abis/*.json --out-dir ./src/helper/typechain"
  },
  "dependencies": {
    "@ethersproject/abi": "^5.7.0",
    "@ethersproject/providers": "^5.7.0",
    "ethers": "^5.7.2",
    "lodash": "^4.17.21",
    "nanoid": "^3.3.6",
    "pg": "^8.10.0",
    "pino": "^8.11.0",
    "slugify": "^1.6.6",
    "typeorm": "^0.3.15"
  },
  "devDependencies": {
    "@typechain/ethers-v5": "^9.0.0",
    "@types/lodash": "^4.14.194",
    "rimraf": "^3.0.2",
    "tsc-alias": "^1.8.5",
    "tslib": "^2.5.0",
    "typechain": "^7.0.1",
    "typescript": "^5.0.4"
  }
}
