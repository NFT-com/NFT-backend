{
  "name": "@nftcom/gql",
  "version": "1.0.0",
  "description": "NFT.com's GraphQL server",
  "main": "dist/index",
  "types": "dist/index",
  "scripts": {
    "prebuild": "npm run gql:gen",
    "build": "npm run clean && npm run compile && npm run copy:schema",
    "clean": "rimraf dist tsconfig.tsbuildinfo",
    "compile": "tsc && tsc-alias",
    "copy:schema": "cp -r ./src/schema/*.graphql ./dist/schema/",
    "dev": "doppler run -- concurrently -k -r \"npm:gql:gen -- --watch\" \"nodemon\"",
    "test": "jest --coverage",
    "testlocal": "doppler run `jest --detectOpenHandles`",
    "test:vscode": "jest --detectOpenHandles",
    "gql:gen": "graphql-codegen",
    "signature:gen": "ts-node script/gen-signature.ts",
    "start": "node -r source-map-support/register ./dist/index",
    "gqldoc": "nodemon -x 'spectaql' -t ../../docs/public -D",
    "gqldoc-old": "graphdoc -e http://localhost:10010/graphql -o ./../../docs/documentation -f"
  },
  "dependencies": {
    "@alch/alchemy-web3": "^1.2.5",
    "@apollo/utils.keyvadapter": "^1.1.0",
    "@graphql-tools/merge": "^8.2.0",
    "@hashgraph/sdk": "^2.7.1",
    "@keyv/redis": "^2.3.6",
    "@sendgrid/mail": "^7.6.0",
    "@sentry/node": "^6.16.1",
    "@sentry/tracing": "^6.16.1",
    "@types/sharp": "^0.30.1",
    "apollo-server-core": "^3.9.0",
    "apollo-server-express": "^3.9.0",
    "aws-sdk": "^2.1038.0",
    "axios": "^0.21.4",
    "bull": "^4.2.0",
    "cors": "^2.8.5",
    "crypto-random-string": "^3.3.1",
    "date-fns": "^2.25.0",
    "dotenv": "^10.0.0",
    "envfile": "^6.17.0",
    "eth-balance-checker": "^0.1.2",
    "ethereumjs-abi": "^0.6.8",
    "ethers": "5.6.4",
    "express": "^4.17.2",
    "graphql": "^15.8.0",
    "graphql-rate-limit-directive": "2.0.0",
    "graphql-resolvers": "^0.4.2",
    "graphql-scalars": "^1.13.1",
    "graphql-upload": "^13.0.0",
    "image-to-base64": "^2.2.0",
    "ioredis": "^5.0.6",
    "joi": "^17.4.2",
    "keyv": "^4.3.0",
    "kill-port": "^1.6.1",
    "lodash": "^4.17.21",
    "path": "^0.12.7",
    "sharp": "^0.30.3",
    "source-map-support": "^0.5.21",
    "spectaql": "^1.4.1",
    "typeorm": "0.2.41",
    "typesense": "^1.2.1",
    "web3": "^1.7.3"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.2.1",
    "@graphql-codegen/typescript": "^2.2.4",
    "@types/bull": "^3.15.7",
    "@types/graphql-resolvers": "^0.3.1",
    "@types/jest": "^28.1.1",
    "@types/lodash": "^4.14.176",
    "@types/node": "^16.11.7",
    "@types/supertest": "^2.0.12",
    "concurrently": "^6.3.0",
    "graphql-markdown": "^6.0.0",
    "jest": "^28.1.1",
    "nodemon": "^2.0.14",
    "rimraf": "^3.0.2",
    "supertest": "^6.2.2",
    "ts-jest": "^28.0.5",
    "ts-node": "^10.4.0",
    "tsc-alias": "^1.4.1",
    "tsconfig-paths": "^3.11.0",
    "typescript": "^4.4.4"
  }
}
